@model ASCMS.Domain.Domain.Content.Page
@using ASCMS.Domain.Domain.Content
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    @foreach (PageRow pr in Model.PageRows)
    {
        foreach (Module pm in pr.RowModules)
        {
            @Html.Action("PageModule", pm);
        }
    }
</body>
</html>
